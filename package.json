{
  "name": "clockinout-system",
  "version": "1.0.0",
  "description": "Employee clock in/out system for store management",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "render-build": "cd server && npm install",
    "render-start": "cd server && npm start"
  },
  "keywords": [
    "employee",
    "clockin",
    "clockout",
    "timesheet",
    "store"
  ],
  "author": "Your Store",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "moment": "^2.29.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
